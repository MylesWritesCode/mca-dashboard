{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "docker": {
      "outputs": [""]
    },
    "migrate:dev:up": {
      "outputs": [""]
    },
    "migrate:dev:down": {
      "outputs": [""]
    },
    "migrate:prod:up": {
      "outputs": [""]
    },
    "storybook": {
      "outputs": [""]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": [
    ".env"
  ]
}
