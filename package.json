{
  "name": "metadmin",
  "version": "0.0.5",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md,css}\"",
    "docker:dev": "docker compose -f .docker/development.yml --env-file .env",
    "docker:dbs": "docker compose -f .docker/databases.yml --env-file .env",
    "docker:prod": "docker compose -f .docker/production.yml --env-file .env"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/material": "^5.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@testing-library/cypress": "^8.0.3",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^17.0.45",
    "@types/webpack": "^5.28.0",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "cypress": "^10.3.0",
    "cypress-social-logins": "^1.14.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-css-order": "^1.2.0",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "sass": "^1.53.0",
    "tailwindcss": "^3.1.4",
    "turbo": "^1.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0"
  },
  "engines": {
    "npm": ">=8",
    "node": ">=16"
  },
  "packageManager": "yarn@3.2.0"
}
