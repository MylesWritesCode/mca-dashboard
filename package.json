{
  "name": "metadmin",
  "version": "0.0.5",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "assets/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md,css}\"",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "docker:dev": "docker compose -f .docker/development.yml --env-file .env up",
    "docker:dbs": "docker compose -f .docker/databases.yml --env-file .env up",
    "docker:prod": "docker compose -f .docker/production.yml --env-file .env up",
    "docker:down": "docker compose down",
    "migrate:dev:up": "turbo run migrate:dev:up",
    "migrate:dev:down": "turbo run migrate:dev:down",
    "migrate:prod:up": "turbo run migrate:prod:up"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/manager-webpack5": "^6.5.6",
    "@storybook/react": "^6.5.6",
    "@storybook/testing-library": "^0.0.11",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^17.0.38",
    "@types/webpack": "^5.28.0",
    "babel-loader": "^8.2.5",
    "prettier": "^2.6.2",
    "prettier-plugin-css-order": "^1.2.0",
    "react-icons": "^4.4.0",
    "sass": "^1.52.1",
    "turbo": "^1.2.16",
    "typescript": "^4.7.2",
    "webpack": "^5.73.0"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "@types/d3": "^7.4.0",
    "chart.js": "^3.8.0",
    "d3": "^7.4.4",
    "lodash.debounce": "^4.0.8"
  }
}
