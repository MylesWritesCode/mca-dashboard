{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "jsxSingleQuote": false,
  "quoteProps": "consistent",
  "printWidth": 120,
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "order": "concentric-css",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "ui",
    "^ui/(.*)$",
    "^[./]",
    "config",
    "^config/(.*)$"
  ],
  "importOrderSeparation": true
}